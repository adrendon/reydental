@import url('http://fonts.googleapis.com/css?family=Raleway:400,300,500,600,800|Roboto+Slab');

@font-face {
	font-family: 'icomoon';
	src:url('fonts/icomoon.eot?m3q3h6');
	src:url('fonts/icomoon.eot?#iefixm3q3h6') format('embedded-opentype'),
		url('fonts/icomoon.woff?m3q3h6') format('woff'),
		url('fonts/icomoon.ttf?m3q3h6') format('truetype'),
		url('fonts/icomoon.svg?m3q3h6#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
